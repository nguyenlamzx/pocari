package com.sff.facebook.data {	import com.sff.facebook.utils.DateFormatter;		public class FBDataUser extends FBAbstractData {				public function FBDataUser(pData:Object = null) {			super(pData);		}				public function get lastName():String {			return objData.last_name;		}				public function get firstName():String {			return objData.first_name;		}				public function get link():String {			return objData.link;		}				public function get birthday():String {			return objData.birthday;		}				public function get hometown():Object {			return objData.hometown;		}				public function get location():Object {			return objData.location;		}				public function get relationshipStatus():String {			return objData.relationship_status;		}				public function get email():String {			return objData.email;		}				public function get gender():String {			return objData.gender;		}				public function get website():String {			return objData.website;		}				public function get religion():String {			return objData.religion;		}				public function get work():String {			return objData.work;		}				public function get education():String {			return objData.education;		}				public function get political():String {			return objData.political;		}				public function get timezone():String {			return objData.timezone;		}				public function get about():String {			return objData.about;		}				public function get significantOther():String {			return objData.significantOther;		}				public function get interestedIn():String {			return objData.interestedIn;		}				public function get meetingFor():String {			return objData.meetingFor;		}				public function get verified():Boolean {			return objData.verified;		}				public function get updatedTime():Date {			return DateFormatter.getDate(objData.updated_time);		}				public function get name():String {			return firstName || lastName ? firstName + " " + lastName : objData.name;		}				override public function toString():String {			return '[FBDataUser id="' + id + '" name="' + name + '"]';		}	}}