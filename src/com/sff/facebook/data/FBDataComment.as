package com.sff.facebook.data {		import com.sff.facebook.utils.DateFormatter;		public class FBDataComment extends FBAbstractData { 				protected var fbdUserFrom: FBDataUser;				public function FBDataComment( pData : Object = null ) {			super( pData );						fbdUserFrom = new FBDataUser( objData.from );		}				public function get from() : FBDataUser { 			return fbdUserFrom; 		}				public function get message() : String { 			return objData.message; 		}				public function get createdTime() : Date { 			return DateFormatter.getDate( objData.created_time ); 		}					override public function toString() : String {			return  '[FBDataComment id="' + id + '"]';		}		}	}