package com.sff.facebook.data {	import com.sff.facebook.utils.DateFormatter;		public class FBDataContent extends FBAbstractData {				protected var arrComments 	:Array;		protected var fbdUserFrom	:FBDataUser;				public function FBDataContent( pData : Object = null ) {			super( pData );		}				public function get from() : FBDataUser {			if ( !objData.from ) {				return null;			}						if ( !fbdUserFrom ) {				fbdUserFrom = new FBDataUser( objData.from );			}						return fbdUserFrom;		}				public function get comments() : Array { 			return arrComments || null; 		}				public function get name() : String { 			return objData.name; 		}					public function get icon() : String { 			return objData.icon; 		}					public function get createdTime() : Date { 			return DateFormatter.getDate( objData.created_time ); 		}					public function get updatedTime() : Date { 			return DateFormatter.getDate( objData.updated_time );		}			}	}