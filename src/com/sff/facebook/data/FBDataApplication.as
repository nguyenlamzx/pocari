package com.sff.facebook.data {	public class FBDataApplication extends FBAbstractData {				public function FBDataApplication( pData : Object = null ) {			super( pData );		}				public function get name() : String { return objData.name; };		public function get link() : String { return objData.link; };		public function get description() : String { return objData.description; };		public function get category() : String { return objData.category; };				public override function toString() : String {			return  '[FBDataApplication id="'+id+'" name="'+name+'"]';		}		}	}