package com.sff.facebook.data {		import com.sff.facebook.utils.DateFormatter;	/**	 * @author Manu Bonnet	 * @project TEST_fbconnect	 * @date 25 mai 10	 */	 	public class FBDataEvent extends FBAbstractData	{				protected var _owner : FBDataUser;				function FBDataEvent( pData : Object = null )		{			super( pData );		}				public function get name() : String { return objData.name; };		public function get description() : String { return objData.description; };		public function get startTime() : Date { return DateFormatter.getDate( objData.start_time ); };		public function get endTime() : Date { return DateFormatter.getDate( objData.end_time ); };		public function get updatedTime() : Date { return DateFormatter.getDate( objData.updated_time ); };		public function get location() : String { return objData.location; };		public function get venue() : Object { return objData.venue; };		public function get privacy() : String { return objData.privacy; };				public function get owner() : FBDataUser		{			if( !objData.owner ) return null;						if( !_owner ) _owner = new FBDataUser( objData.owner );			return _owner;		};				/**		*		*/		public override function toString() : String		{			return  '[FBDataEvent id="'+id+'"]';		}		}	}